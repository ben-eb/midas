{
  "name": "midas",
  "version": "1.2.1",
  "description": "Syntax highlighter based on PostCSS.",
  "bin": {
    "midas": "bin/cmd.js"
  },
  "main": "dist/index.js",
  "files": [
    "bin",
    "LICENSE-MIT",
    "dist"
  ],
  "scripts": {
    "pretest": "eslint src",
    "prepublish": "del-cli dist && cross-env BABEL_ENV=publish babel src --out-dir dist --ignore /__tests__/ && node theme.js",
    "test": "ava src/__tests__/*.js"
  },
  "keywords": [
    "css",
    "highlight",
    "postcss"
  ],
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.15.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "cross-env": "^1.0.7",
    "del-cli": "^0.2.0",
    "eslint": "^2.0.0",
    "eslint-config-cssnano": "^2.0.0",
    "js-yaml": "^3.4.1",
    "postcss-map": "^0.8.0"
  },
  "homepage": "https://github.com/ben-eb/midas",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/midas",
  "dependencies": {
    "html-tags": "^1.1.1",
    "minimist": "^1.2.0",
    "postcss": "^5.0.4",
    "postcss-selector-parser": "^2.0.0",
    "postcss-value-parser": "^3.3.0",
    "read-file-stdin": "^0.2.0",
    "write-file-stdout": "0.0.2"
  },
  "ava": {
    "require": "babel-core/register"
  },
  "eslintConfig": {
    "extends": "cssnano"
  }
}
